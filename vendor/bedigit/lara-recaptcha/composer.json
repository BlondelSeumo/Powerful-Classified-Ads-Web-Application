{
    "name": "bedigit/lara-recaptcha",
    "description": "Advanced reCAPTCHA package for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Bedigit",
            "email": "contact@bedigit.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^7.3",
        "laravel/framework": "^6.20.13|^7.30.3|^8.23"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Bedigit\\ReCaptcha\\": "src/"
        },
        "files": [
            "src/app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "Bedigit\\ReCaptcha\\ReCaptchaServiceProvider"
            ],
            "aliases": {
                "ReCaptcha": "Bedigit\\ReCaptcha\\Facades\\ReCaptcha"
            }
        }
    }
}
